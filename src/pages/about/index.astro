---
import FeedLayout from "../../layouts/Feed.astro";
import { getAllPageData } from '../../utils/getPageData.js'
import { ROUTES } from '../../constants/routes.js'

const data = await getAllPageData()

const page = {
  ...data,
  route: ROUTES.about.href,
}
---
<FeedLayout
  title="About Jhey"
  description="Some things about Jhey Tompkins"
  ogtitle="About Jhey"
  oggradient="7"
  page={page}
/>